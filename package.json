{
  "name": "pi-sync",
  "version": "0.1.3",
  "description": "Auto sync codes to Raspberry Pi by SFTP, inspirated by docker-sync.",
  "main": "bin/index.js",
  "scripts": {
    "babel": "babel src --out-dir bin --source-maps --copy-files",
    "watch": "babel src -d bin -s -w --copy-files",
    "test": "npm run babel && mocha -r intelli-espower-loader",
    "coverage": "nyc mocha",
    "coveralls": "npm run coverage && nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run babel"
  },
  "keywords": [
    "raspberry-pi",
    "sftp",
    "sync"
  ],
  "author": "Jerry Lee",
  "license": "MIT",
  "bin": {
    "pi-sync": "bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jerrywdlee/pi-sync.git"
  },
  "dependencies": {
    "chokidar": "^1.7.0",
    "colors": "^1.1.2",
    "commander": "^2.12.2",
    "ignore-walk": "^3.0.1",
    "lodash": "^4.17.4",
    "node-ssh": "^5.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node5": "^1.2.0",
    "coveralls": "^3.0.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "power-assert": "^1.4.4"
  }
}
